      "package": "com.whisperjournal"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "42ce6a8e-5764-42ee-9c12-5375152a0d48"
      }
    }
